{
   "name": "tic-tac-toe-grow",
   "description": "A variant of Tic tac toe where the board grows.",
   "version": "0.364.0-alpha",
   "license": "Apache-2.0",
   "keywords": [
      "tic tac toe"
   ],
   "scripts": {
      "test": "npm run check-syntax && npm run test-js",
      "check-syntax": "npm run check-html-syntax && npm run check-js-syntax",
      "check-html-syntax": "npx html-validator-cli --file=game.html --verbose",
      "check-js-syntax": "npx standardx --verbose",
      "test-js": "jest"
   },
   "browser": "./game.html",
   "browserslist": [
      "> 5%",
      "last 2 versions",
      "not ie <= 10"
   ],
   "author": {
      "name": "Steven Nguyen",
      "email": "58114641+icecream17@users.noreply.github.com",
      "url": "https://icecream17.github.io/"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/icecream17/tic-tac-toe-grow-for-website.git"
   },
   "bugs": {
      "url": "https://github.com/icecream17/tic-tac-toe-grow-for-website/issues"
   },
   "devDependencies": {
      "@babel/core": "^7.13.10",
      "@babel/eslint-parser": "^7.13.10",
      "@babel/eslint-plugin": "^7.13.10",
      "@babel/plugin-syntax-class-properties": "^7.12.13",
      "@babel/preset-env": "^7.13.10",
      "@types/jest": "^26.2.22",
      "@typescript-eslint/eslint-plugin": "^4.22.0",
      "@typescript-eslint/parser": "^4.22.0",
      "babel-jest": "^26.6.3",
      "eslint": "^7.21.0",
      "eslint-plugin-jest": "^24.3.5",
      "eslint-plugin-import": "^2.22.1",
      "html-validator-cli": "^7.0.1",
      "jest": "^26.6.3",
      "serve": "^11.3.2",
      "standardx": "^7.0.0",
      "ts-jest": "^26.5.5",
      "typescript": "^4.2.4"
   },
   "engines": {
      "npm": "^7.6"
   },
   "standardx": {
      "parser": "@babel/eslint-parser"
   }
}
